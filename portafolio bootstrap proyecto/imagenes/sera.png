<script type='text/javascript'>
    // window.ana_debug = true;

    /**
     * Returns the first child of an HTML element
     * @param {Element} element
     */
    function getFirstChild(element) {
        return element.children[0];
    }

    document.addEventListener('ANAReady', () => {
        wanaLoad();
    });

    const wanaLoad = () => {
        const gptOptions = {
            enableSingleRequest: true,
            sandboxIframe: true,
            collapseEmptyDivs: false,
            globalTargeting: {
                'domain': window.location.hostname,
                'youtube': 'n',
            },
        };

        window.ANAWeb = new ANAWeb({ gptOptions });
    }

    /**
     * Creates an Assembly ad slot
     * @deprecated Use createAssemblyAdSlot instead.
     * By passing an object as parameter instead of individual paramenters, you can
     * easily extend, remove or change the parameters by adding, removing or changing
     * new keys to the object.
     */
    const createSlot = (slotName, elementID, adUnitID, dimensions, refresh = false, maxWidth = undefined) => {
        window[slotName] = window.ANAWeb.createSlot(SlotTypeEnum.GOOGLE, {
            elementID,
            adUnitID,
            dimensions,
            collapseBeforeFill: false,
            collapseIfNoFill: false,
        });

        window[slotName].refresh();

        if (refresh) {
            refreshInterval(slotName, maxWidth);
        }
    }

    /**
     * Creates an Assembly ad slot.
     * @see https://whisper.atlassian.net/wiki/spaces/ASTI/pages/4972118021/2.0.12#createSlot(slotType%2C-slotOptions)-BannerSlot
     * @param { Object } options
     * @param { string } options.slotName A reference of the slot created is stored under window with the slotName as key
     * @param { string } options.elementID The ID of the HTML element the ad created will be placed
     * @param { string } options.adUnitID The GAM ad unit id, eg: `/114106652/wshh_desktop_hp_btf_3`
     * @param { number[][] } options.dimensions Array of arrays to map to supported sizes
     * @param { boolean | undefined } options.refresh If true, the refresh function will be executed for the given slot
     * @param { number | undefined } options.maxWidth ???
     * @param { boolean | undefined } options.collapseBeforeFill If true the element will remain collapsed unless the ad fills
     * @param { boolean | undefined } options.collapseIfNoFill If true the the element will collapse if the ad attempt does not fill
     */
    function createAssemblyAdSlot(options) {
        const {
            slotName,
            elementID,
            adUnitID,
            dimensions,
            refresh = false,
            maxWidth = undefined,
            collapseBeforeFill = false,
            collapseIfNoFill = false,
        } = options;

        if (!slotName || typeof slotName !== "string") {
            throw new Error('slotName (string) is required');
        }

        if (!elementID || typeof elementID !== "string") {
            throw new Error('elementID (string) is required');
        }

        if (!adUnitID || typeof adUnitID !== "string") {
            throw new Error('adUnitID (string) is required');
        }

        if (!dimensions || !Array.isArray(dimensions)) {
            /** @TODO Probably we should do a better validation here */
            throw new Error('dimensions (number[][]) is required');
        }

        window[slotName] = window.ANAWeb.createSlot(SlotTypeEnum.GOOGLE, {
            elementID,
            adUnitID,
            dimensions,
            collapseBeforeFill,
            collapseIfNoFill,
        });

        window[slotName].refresh();

        //window.ANAWeb.refreshAllGoogleSlots();

        if (refresh) {
            refreshInterval(slotName, maxWidth);
        }
    }

    const createMobileSlots = () => {
        var adBlockContainers = [...document.getElementsByClassName("ad-block-container-mobile")];
        adBlockContainers.forEach((container, index) => {
            const element = getFirstChild(container);

            if (element.id === `slot-mobile-${index}`) {
                return;
            }

            element.id = `slot-mobile-${index}`;
            element.style.textAlign = 'center';

            window.createSlot(`slotMobile${index}`, `slot-mobile-${index}`, '/114106652/amino_300x250_atf_mw', [[300, 250], [300, 600]]);
        });
    }

    /**
     * Create ad slots for ad containers between posts items in the Community feed.
     */
    function fillInBetweenPostsAdSlots() {
        var slotContainers = [...document.getElementsByClassName("post-list-item--desktop-ad")];

        slotContainers.forEach((container, index) => {
            const element = getFirstChild(container);
            const elementID = `slot-desktop-${index}`;

            // If the slot is already created, skip it.
            if (element.id === elementID) {
                return;
            }

            element.id = elementID;
            element.style.textAlign = "center";
            const adUnitID = "/114106652/amino_desktop_728x90";
            const adDimensions = [[728, 90], [970, 250], [970, 90], [960, 250], [300, 250]];

            window.createAssemblyAdSlot({
                slotName: `slotDesktop${index}`,
                elementID,
                adUnitID,
                dimensions: adDimensions,
                collapseBeforeFill: false,
                collapseIfNoFill: false,
                refresh: true,
            });
        })
    }

    window.createSlot = createSlot;
    window.createAssemblyAdSlot = createAssemblyAdSlot;
    window.createMobileSlots = createMobileSlots;
    window.fillInBetweenPostsAdSlots = fillInBetweenPostsAdSlots;

    const refreshInterval = (slotName, maxWidth) => {
        window[`${slotName}Refresh`] = setInterval(() => {
            if (maxWidth && window.innerWidth < maxWidth) {
                return;
            }

            window[slotName].refresh();
        }, 10000);
    };

    // Every debounce needs to have it's method because of overplapping
    const scrollListener = (className, id) => window.addEventListener("scroll", () => {
        if (id === 'slot-right-aside') {
            return debounceInvokeRight(className, id);
        }

        debounceInvoke(className, id);
    }, { passive: true });

    const isInViewport = (element, topAdd = false) => {
        const rect = element.getBoundingClientRect();

        if (topAdd) {
            return (rect.bottom >= 50);
        }

        return (
            // header height is approx. 50px;
            rect.top >= 50 &&
            rect.bottom <= (window.innerHeight || document.documentElement.clientHeight)
        );
    }

    const debounce = (func, wait) => {
        let timeout;

        return function executedFunction(...args) {
            const later = () => {
                clearTimeout(timeout);
                func(...args);
            };

            clearTimeout(timeout);
            timeout = setTimeout(later, wait);
        };
    };

    const debounceInvoke = debounce(function (className, id) {
        runOnScroll(className, id);
    }, 1000);

    const debounceInvokeRight = debounce(function (className, id) {
        runOnScroll(className, id);
    }, 1000);

    const runOnScroll = (className, id) => {
        const elementsWithGivenClass = document.getElementsByClassName(className);

        for (let i = 0; i < elementsWithGivenClass.length; i++) {
            const element = elementsWithGivenClass[i];

            if (isInViewport(element)) {
                if (getFirstChild(element).id === id) {
                    break;
                }

                let itemToMove;
                let placeToMove;

                [...elementsWithGivenClass].find((item, index) => {
                    if (getFirstChild(item).id === id) {
                        itemToMove = item;
                        placeToMove = index;
                    }
                });

                element.appendChild(getFirstChild(itemToMove));

                itemToMove.appendChild(getFirstChild(element));

                getFirstChild(element).style.display = "none";

                if (id === 'slot-right-aside') {
                    clearInterval(window.slotRightAsideRefresh);
                    refreshInterval('slotRightAside', 1207);
                    window.slotRightAside.refresh();
                }

                if (id === 'slot-banner') {
                    window.slotBanner.refresh();
                }
            }
        }
    };

    window.scrollListener = scrollListener;
    window.isInViewport = isInViewport;
</script>
